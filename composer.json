{
    "name": "spryker/opentelemetry",
    "type": "library",
    "description": "OpenTelemetry module",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "open-telemetry/api": "*",
        "open-telemetry/sdk": "*",
        "open-telemetry/sem-conv": "*",
        "spryker/kernel": "*",
        "spryker/monitoring-extension": "*",
        "spryker/symfony": "*"
    },
    "require-dev": {
        "open-telemetry/sdk": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {},
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
